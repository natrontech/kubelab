{
  "name": "ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../kubelab-backend && modd",
    "backend": "cd ../kubelab-backend && ./pocketbase serve --publicDir=../kubelab-ui/build",
    "build": "vite build",
    "build:backend": "cd ../kubelab-backend && go build",
    "typegen": "pocketbase-typegen --db ../kubelab-backend/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.25.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/marked": "^5.0.0",
    "@types/node": "^20.11.30",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "daisyui": "^4.10.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "flowbite": "^2.3.0",
    "flowbite-svelte": "^0.44.18",
    "flowbite-svelte-icons": "^0.4.4",
    "mdsvex": "^0.10.6",
    "pocketbase": "^0.21.1",
    "postcss": "^8.4.38",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^4.2.1",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.1",
    "svelte-splitpanes": "^0.7.14",
    "tailwind-scrollbar": "^3.0.4",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.6.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.2.8",
    "@picocss/pico": "^1.5.10",
    "@sveltejs/adapter-node": "^1.2.1",
    "@sveltejs/adapter-static": "^2.0.2",
    "@webcontainer/api": "^1.1.4",
    "chart.js": "^4.3.0",
    "lucide-svelte": "^0.265.0",
    "marked": "^5.0.4",
    "pocketbase-typegen": "^1.2.1",
    "prismjs": "^1.29.0",
    "svelte-confetti": "^1.3.2",
    "svelte-french-toast": "^1.2.0",
    "svelte-icons-pack": "^2.1.0",
    "svelte-local-storage-store": "^0.6.4",
    "svelte-markdown": "^0.2.3",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.7.0"
  }
}
